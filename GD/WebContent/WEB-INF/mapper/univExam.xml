<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="univExam">
	<insert id="write" parameterType="java.util.HashMap">
		insert into univexam values(#{title}, #{content},  #{id}, #{name}, now())
	</insert>
	<select id="univExam" resultType="java.util.HashMap">
		select * from univexam order by day desc limit 10 offset #{page}
	</select>
	<select id="count" resultType="java.lang.Integer">
		select count(*) from univexam
	</select>
</mapper>